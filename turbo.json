{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**/*.d.ts",
        "dist/**/*.mjs",
        "dist/**/*.js",
        "dist/**/*.css",
        ".next/**",
        "index.js",
        "ssg.js",
        "loader.js",
        ".cache"
      ]
    },
    "start": {
      "cache": false
    }
  }
}
